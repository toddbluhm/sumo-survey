{
  "name": "sumo-survey",
  "version": "1.0.0",
  "description": "A Question and Survey webapp for sumo coding challenge",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel build-client start-api-server start-web-server",
    "build-client": "webpack-dev-server --config './webpack/webpack.config.js' --colors --display-error-details",
    "start-api-server": "nodemon api --watch api --ext js,jsx",
    "start-web-server": "nodemon server --watch server --watch app --ext js,jsx",
    "production": "npm-run-all production-build-client production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "production-api-server": "node ./api --production",
    "production-web-server": "node ./server --production",
    "production-server": "npm-run-all --parallel production-api-server production-web-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "survey",
    "question",
    "answer",
    "sumo",
    "code challenge"
  ],
  "author": "Todd Bluhm",
  "license": "MIT",
  "dependencies": {
    "api-error-handler": "^1.0.0",
