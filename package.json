{
  "name": "sumo-survey",
  "version": "1.0.0",
  "description": "A Question and Survey webapp for sumo coding challenge",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all prepare-server-build start-development-workflow",
    "prepare-server-build": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "start-development-workflow": "npm-run-all --parallel build-client build-server start-server",
    "build-client": "node ./webpack/webpack-dev-server.entry.js",
    "build-server": "webpack --config ./webpack/webpack.config.server.entry.js --watch --colors --display-error-details",
    "start-server": "npm-run-all --parallel start-api-server start-web-server",
    "start-api-server": "nodemon api --watch ./api",
    "start-web-server": "nodemon server --delay 2 --watch ./build/server --watch ./server --watch ./app",
    "dev-web-server": "npm-run-all --parallel build-server start-web-server",
    "production": "npm-run-all production-build-client production-build-server production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./webpack/webpack.config.client.entry.js",
    "production-build-server": "webpack --colors --display-error-details --config ./webpack/webpack.config.server.entry.js",
    "production-api-server": "node ./api --production",
    "production-web-server": "node ./server --production",
    "production-server": "npm-run-all --parallel production-api-server production-page-server production-web-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "survey",
    "question",
    "answer",
    "sumo",
    "code challenge"
  ],
  "author": "Todd Bluhm",
  "license": "MIT",
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-bluebird": "^0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-http-proxy": "^0.9.0",
    "express-mysql-session": "^1.1.1",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-ponyfill": "^1.0.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.15.2",
    "morgan": "^1.7.0",
    "npm-run-all": "^2.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-connect": "git+ssh@github.com:toddbluhm/redux-connect.git#immutable-store-support",
    "redux-immutable": "^3.0.6",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "scrypt": "^6.0.3",
    "sequelize": "^4.0.0-0",
    "universal-webpack": "^0.1.33",
    "validator": "^5.5.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "faker": "^3.1.0",
    "install": "^0.8.1",
    "npm": "^3.10.5",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
