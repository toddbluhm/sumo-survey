    "babel-preset-es2017": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-device": "^0.4.2",
    "express-jwt-permissions": "^0.2.2",
    "fetch-ponyfill": "^1.0.0",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.1.9",
    "material-ui": "^0.15.3",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "npm-run-all": "^2.3.0",
